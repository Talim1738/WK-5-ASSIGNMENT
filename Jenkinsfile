pipeline {
    agent any

    stages {
        stage('Building Stage') {
            steps {
                echo 'Building the project...'
            }
        }
        stage('Testing Stage') {
            steps {
                echo 'Running tests...'
            }
    
        }
        stage('Deploy Stage') {
            steps {
                echo 'Deploying application...'
            }
        }
        stage('Releasing Stage') {
            steps {
                echo 'Releasing the Application...'
            }
        }
    }
}
